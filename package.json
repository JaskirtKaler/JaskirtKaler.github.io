{
  "name": "jaskirt_website",
  "version": "1.0.0",
  "description": "Jaskirt website fresh start",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass scss/main.scss style.css -w",
    "watch:sass": "node-sass scss/main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass style.prefix.css style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Jaskirt Kaler",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "live-server": "^1.2.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}
